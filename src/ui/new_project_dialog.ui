<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <requires lib="gtk" version="4.0"/>
  <requires lib="adw" version="1.0"/>
  <template class="NewProjectDialog" parent="AdwAlertDialog">
    <property name="heading">Create a New Project</property>
    <property name="default-response">create</property>
    <response id="cancel" label="_Cancel" appearance="destructive"/>
    <response id="create" label="_Create" appearance="suggested" default="true"/>
    <property name="extra-child">
        <object class="AdwPreferencesPage">
            <child>
                <object class="AdwPreferencesGroup">
                <child>
                    <object class="AdwEntryRow" id="project_name_entry">
                    <property name="title">Project Name</property>
                    </object>
                </child>
                <child>
                    <object class="AdwComboRow" id="template_combo">
                    <property name="title">Template</property>
                    </object>
                </child>
                </object>
            </child>
        </object>
    </property>
  </template>
</interface>
